import tkinter as tk
from tkinter import filedialog
import pandas as pd
import numpy as np
from tabulate import tabulate
header = []
header = ['FlightID', 'Flight#', 'Origin', 'Destination', 'Dep. Time','Arrival time', 'Equipment', 'Mileage', 'Release time', 'Status','Desk#']
results = []
layout = tabulate(results, headers = header)
print(layout)
page3b= tk.Tk()
tk.Label(page3b, text=layout).grid(row=0, column=0)
page3b.mainloop()
